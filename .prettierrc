{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "none",
  "jsxBracketSameLine": false,
  "semi": true,
  "requirePragma": false,
  "insertPragma": false,
  "useTabs": false,
  "tabWidth": 4,
  "bracketSpacing": false,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    }
  ]
}
